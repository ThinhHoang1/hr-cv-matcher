{
  "name": "CV Upload (Auto Generate Embeddings)",
  "nodes": [
    {
      "parameters": {
        "jsCode": "// Extract all files from webhook\nconst items = [];\n\nfor (const item of $input.all()) {\n  if (item.binary) {\n    for (const key in item.binary) {\n      const file = item.binary[key];\n      items.push({\n        json: {\n          fileName: file.fileName,\n          mimeType: file.mimeType\n        },\n        binary: {\n          data: file\n        }\n      });\n    }\n  }\n}\n\nreturn items;"
      },
      "id": "997a425c-087c-40d6-8d7b-6119966073f2",
      "name": "Extract All Files",
      "type": "n8n-nodes-base.code",
