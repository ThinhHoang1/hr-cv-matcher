      ]
    },
    {
      "parameters": {
        "jsCode": "// Aggregate all parsed candidates\nconst candidates = $input.all().map(item => item.json);\n\nreturn [{\n  json: {\n    success: true,\n    processed: candidates.length,\n    candidates: candidates\n  }\n}];"
      },
