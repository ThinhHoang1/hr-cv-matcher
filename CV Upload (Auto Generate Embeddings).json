      ]
    },
    {
      "parameters": {
        "jsCode": "// Prepare text for Gemini\nconst extractedText = $json.data || $json.text || '';\n\nreturn [{\n  json: {\n    fileName: $json.fileName,\n    extractedText: extractedText\n  }\n}];"
      },
